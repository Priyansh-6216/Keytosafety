@page

@model KeyToSaftey.Pages.User.webscrappingModel
@{
    Layout = "~/Pages/Shared/_Layout1.cshtml";

}


        <html>
        <head>
            <title>Data Scraping</title>
            <style>
                img {
                    width: 100px;
                    height: auto;
                }
            </style>
        </head>
        <body style="font-family:Cambria">
            <div class="container main-container headerOffset">
                <div class="row">
                    @foreach (var item in Model.products)
                    {
                        <div class="item col-sm-4 col-lg-4 col-md-4 col-xs-6">
                            <div class="product" style="text-align:center">

                                <div class="image"  style="width:150px;height:150px;">
                                    <div>
                                        <div> @Html.Raw(@item.Img)</div>

                                    </div>
                                </div>
                                <div class="description">
                                    <h4> @item.Name</h4>

                                    <div class="grid-description">
                                        <p> @item.Rating</p>
                                    </div>
                                </div>
                                <div class="price"><span>@item.Price</span></div>
                                <div class="action-control">
                                    <a href="@item.Link" target="blank" class="btn btn-primary">
                                        
                                            <span >
                                                View Product
                                            </span>
                                        </a>
                                </div>
                            </div>
                        </div>

                    }




                </div>
            </div>
</body>
    </html>
