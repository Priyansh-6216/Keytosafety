@page
@model KeyToSaftey.Pages.Admin.ASignupModel

@{
    ViewData["Title"] = "ASignup";
    Layout = "~/Pages/Shared/_Layout1.cshtml";
}

<h2>ASignup</h2>

<h4>AdminSignup</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AdminSignup.Id" class="control-label"></label>
                <input asp-for="AdminSignup.Id" class="form-control" />
                <span asp-validation-for="AdminSignup.Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdminSignup.AfullName" class="control-label"></label>
                <input asp-for="AdminSignup.AfullName" class="form-control" />
                <span asp-validation-for="AdminSignup.AfullName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdminSignup.Amobile" class="control-label"></label>
                <input asp-for="AdminSignup.Amobile" class="form-control" />
                <span asp-validation-for="AdminSignup.Amobile" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdminSignup.Aemail" class="control-label"></label>
                <input asp-for="AdminSignup.Aemail" class="form-control" />
                <span asp-validation-for="AdminSignup.Aemail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdminSignup.Apassword" class="control-label"></label>
                <input asp-for="AdminSignup.Apassword" class="form-control" />
                <span asp-validation-for="AdminSignup.Apassword" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

